 {\r\n            clearInterval(this.interval);\r\n            this.timeOverCB();\r\n        }\r\n        this.update();\r\n    }\r\n\r\n    stop() {\r\n        clearInterval(this.interval);\r\n        this.time = 0;\r\n        this.update();\r\n    }\r\n\r\n    pause() {\r\n        clearInterval(this.interval);\r\n    }\r\n\r\n    continue() {\r\n        if (this.time !== 0)\r\n            this.interval = setInterval(() => this.sec(), 1000);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"d11c2e5f198c6d5e49fa880cc80d7089","cacheData":{"env":{}}}